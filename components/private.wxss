/* 线 */

.border {
  border: 1rpx solid;
}

.b {
  position: relative;
}

.b::after {
  content: "";
  pointer-events: none;
  box-sizing: border-box;
  position: absolute;
  width: 500%;
  height: 500%;
  transform: scale(0.2, 0.2);
  left: 0;
  top: 0;
  border: 1rpx solid #797878;
  transform-origin: 0 0;
  border-radius: 10rpx;
}

.br {
  position: relative;
}

.br::after {
  content: "";
  pointer-events: none;
  box-sizing: border-box;
  position: absolute;
  width: 500%;
  height: 500%;
  transform: scale(0.2, 0.2);
  left: 0;
  top: 0;
  border-right: 1rpx solid #797878;
  transform-origin: 0 0;
}

.bb {
  position: relative;
}

/* #b6c2ce */

.bb::after {
  content: "";
  pointer-events: none;
  box-sizing: border-box;
  position: absolute;
  width: 500%;
  height: 500%;
  transform: scale(0.2, 0.2);
  left: 0;
  top: 0;
  border-bottom: 1rpx solid #b6c2ce;
  transform-origin: 0 0;
}
/* 阴影 (推荐使用伪元素方法)*/
.shadow-6{
   /* 上下、左右、阴影长度、颜色 */
  box-shadow: 3rpx 5rpx 6rpx red;  
  
}

.shadow{
  position: relative;
}
.shadow::before{
  content: "";
  display: block;
  background-color: inherit;
  filter: blur(8rpx);
  position: absolute;
  width: 100%;
  height: 100%;
  top: 10rpx;
  left: 10rpx;
  z-index: -1;
  opacity: 0.4;
  border-radius: inherit;
  transform: scale(1, 1);
}



/*弹窗 隐藏、显示 */

.hiddenview {
  opacity: 0;
  transform: scale(1.18);
  transition: all 0.3s ease-in 0s;
  pointer-events: none;
}

.showview {
  opacity: 1;
  transform: scale(1);
  transition: all 0.3s ease-in 0s;
}

