}

/* 圆形 */

.circle {
  width: 15rpx;
  height: 15rpx;
  border-radius: 50%;
}

/* 红心形状 */

.heart {
  position: relative;
  width: 12.5px;
  height: 11.25px;
}

.heart:before, .heart:after {
  position: absolute;
  content: "";
  left: 6.25px;
  top: 0;
  width: 6.25px;
  height: 10px;
  background: red;
  border-radius: 6.25px 6.25px 0 0;
  transform: rotate(-45deg);
  transform-origin: 0 100%;
}

.heart:after {
  left: 0;
  transform: rotate(45deg);
  transform-origin: 100% 100%;
}

.heartG {
  position: relative;
  width: 12.5px;
  height: 11.25px;
}

.heartG:before, .heartG:after {
  position: absolute;
  content: "";
  left: 6.25px;
  top: 0;
  width: 6.25px;
  height: 10px;
  background: #a3a3a3;
  border-radius: 6.25px 6.25px 0 0;
  transform: rotate(-45deg);
  transform-origin: 0 100%;
}

.heartG:after {
  left: 0;
  transform: rotate(45deg);
  transform-origin: 100% 100%;
}

/* 超出自动 */

.auto {
  overflow: auto;
}

.hidden {
  overflow: hidden;
}

/* 超出部分使用省略号 */

.line-1 {
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  word-break: break-all;
}

/* 旋转2D 、3D  顺时针方向*/

.rotate-90 {
  transform: rotate(90deg);
  transition: all 0.3s;
}

.rotate-180 {
  transform: rotate(180deg);
  transition: all 0.3s;
}

.rotate-270 {
  transform: rotate(270deg);
  transition: all 0.3s;
}

.rotate-360 {
  transform: rotate(360deg);
  transition: all 0.3s;
}

.rotate3d-180 {
  transform: rotate3d(180deg);
  transition: all 0.3s;
}

/* 音乐动画 */

@keyframes circle {
  0% {
    transform: rotate(0deg) scale(1.1);
    background-color: red;
  }

  25% {
    transform: rotate(90deg) scale(0.8);
    background-color: blue;
  }

  50% {
    transform: rotate(180deg) scale(1.1);
    background-color: orange;
  }

  75% {
    transform: rotate(270deg) scale(0.8);
    background-color: blue;
  }

  100% {
    transform: rotate(360deg) scale(1.1);
    background-color: red;
  }
}

